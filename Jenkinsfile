pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code'
            }
        }
         stage('UniTest') {
            steps {
                echo 'UnitTest the code'
            }
        }
         stage('Package') {
            steps {
                echo 'Package the code'
            }
        }
    }
}
